    "eslint-plugin-tailwindcss": "^3.17.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/config-inspector": "^0.5.4",
    "@release-it/conventional-changelog": "^8.0.2",
    "eslint": "^9.12.0",
    "husky": "^9.1.6",
    "release-it": "^17.8.2"
  }
