{
  "name": "@stefanobartoletti/eslint-config",
  "type": "module",
  "version": "3.0.6",
  "packageManager": "pnpm@9.12.2",
  "description": "",
  "author": "Stefano Bartoletti (https://github.com/stefanobartoletti/)",
  "license": "MIT",
  "repository": "stefanobartoletti/eslint-config",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "main": "index.js",
  "scripts": {
