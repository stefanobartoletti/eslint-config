{
  "name": "@stefanobartoletti/eslint-config",
  "type": "module",
  "version": "3.0.1",
  "packageManager": "pnpm@9.9.0",
  "description": "",
  "author": "Stefano Bartoletti (https://github.com/stefanobartoletti/)",
  "license": "MIT",
  "repository": "stefanobartoletti/eslint-config",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "config-inspector",
    "release": "release-it",
    "release:beta": "release-it --no-git.requireBranch --preRelease=beta",
    "commitlint": "commitlint --edit",
    "prepare": "husky"
  },
  "peerDependencies": {
    "eslint": ">=9.9.1"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.23.2",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-tailwindcss": "^3.17.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/config-inspector": "^0.5.4",
    "@release-it/conventional-changelog": "^8.0.1",
    "eslint": "^9.9.1",
    "husky": "^9.1.1",
    "release-it": "^17.6.0"
  }
